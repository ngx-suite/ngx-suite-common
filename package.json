{
  "name": "ngx-suite-common-demo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-demo": "npm run lint && npm run test && npm run build-demo",
    "build-lib": "ng build ngx-suite-common-lib && npm run copy-lib-assets",
    "build-lib-for-publish": "npm run lint && npm run test && npm run build-lib",
    "watch": "ng build --watch --configuration development",
    "test": "jest --passWithNoTests",
    "e2e": "cypress run --e2e",
    "e2e:open": "cypress open --e2e",
    "lint": "npm run lint:ts && npm run lint:styles",
    "lint:fix": "npm run lint:ts:fix && npm run lint:styles:fix",
    "lint:ts": "eslint \"src/**\" \"projects/**\" --quiet",
    "lint:ts:fix": "eslint \"src/**\" \"projects/**\" --quiet --fix",
    "lint:styles": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\"",
    "lint:styles:fix": "stylelint \"src/**/*.scss\" \"projects/**/*.scss\" --fix",
    "lint-staged": "lint-staged",
    "prepare": "husky",
    "install": "npm i --force",
    "copy-lib-assets": "shx cp -r \"projects/ngx-suite-common/**/assets/\" \"dist/ngx-suite-common/\" && shx cp -r \"projects/ngx-suite-common/**/styles/\" \"dist/ngx-suite-common/\"",
    "pre-pull-request": "npm run lint && npm run test",
    "-------------------------NGX-SUITE--DEVKIT-COMMANDS--------------------------":  "",
    "ns::sync-angular-env": "ng g @ngx-suite/devkit:sync-ang-env --source-dir-path=projects/ngx-suite-common --parent-lib-alias=@ngx-suite/common",
    "ns::clean-up-ang-env": "ng g @ngx-suite/devkit:clean-up-ang-env --project-name=ngx-suite-common-demo",
    "ns::new-entry-point": "ng g @ngx-suite/devkit:new-entry-point --base-path=ngx-suite-common/components/icon --ts-path-fragment-aliases=ngx-suite-,@ngx-suite/"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "20.2.9",
    "@angular/common": "20.3.6",
    "@angular/compiler": "20.3.6",
    "@angular/core": "20.3.6",
    "@angular/forms": "20.3.6",
    "@angular/material": "20.2.9",
    "@angular/material-moment-adapter": "20.2.9",
    "@angular/platform-browser": "20.3.6",
    "@angular/router": "20.3.6",
    "@ngx-suite/devkit": "20.0.0",
    "@tailwindcss/postcss": "4.1.14",
    "rxjs": "7.8.2",
    "tailwindcss": "4.1.14",
    "tslib": "2.3.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.3.6",
    "@angular/cli": "20.3.6",
    "@angular/compiler-cli": "20.3.6",
    "@angular/platform-browser-dynamic": "20.3.6",
    "@eslint/js": "9.16.0",
    "@stylistic/eslint-plugin": "2.12.1",
    "@stylistic/stylelint-plugin": "3.1.1",
    "@types/eslint__js": "8.42.3",
    "@types/jest": "30.0.0",
    "@types/lodash-es": "4.17.12",
    "@types/node": "18.19.67",
    "angular-eslint": "19.0.2",
    "cypress": "14.3.3",
    "eslint": "9.16.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unused-imports": "4.1.4",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-preset-angular": "15.0.3",
    "lint-staged": "15.2.11",
    "ng-packagr": "20.3.0",
    "postcss": "8.4.49",
    "shx": "0.4.0",
    "stylelint": "16.11.0",
    "stylelint-config-alphabetical-order": "1.0.0",
    "stylelint-config-sass-guidelines": "12.1.0",
    "stylelint-scss": "6.10.0",
    "ts-node": "10.9.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.1"
  }
}
